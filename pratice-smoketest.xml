<!-- Run xyzBankTest(), totalPrice() and createPatient() methods as a grup test  -->

<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Practice" verbose="1" parallel="methods" thread-count="3">
    <test name="Practice Smoke Test" >
        <groups>
            <run>
                <include name="smoke-test"/>
            </run>
        </groups>
        <packages>
            <package name="dt_project.tests.*"> </package>
        </packages>

    </test>
</suite>






